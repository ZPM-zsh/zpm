#!/usr/bin/env zsh

local Plugin="$1"
local Plugin_path="$2"
local Plugin_functions_path="$3"
local autoload_files="$4"

if [[ "$Plugin"  == *",autoload:"* ]]; then
  local _ZPM_autoload_str=${Plugin##*,autoload}
  _ZPM_autoload_str=${_ZPM_autoload_str%%\,*}
  autoload_files="${autoload_files}${_ZPM_autoload_str}"
fi

echo "$autoload_files"
